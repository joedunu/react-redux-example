{
  "name": "y",
  "version": "0.0.1",
  "description": "Example implementation of react-redux",
  "main": "index.js",
  "engines" : { "node" : ">=6.9.2" },
  "scripts": {
    "start": "webpack-dev-server",
    "test": "NODE_ENV=test jest && istanbul check-coverage",
    "lint": "standard --verbose | snazzy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedunu/react-redux-example.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Anuradha Dunuhappawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joedunu/react-redux-example/issues"
  },
  "homepage": "https://github.com/joedunu/react-redux-example#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "istanbul": "^0.4.5",
    "jest-babel-istanbul": "^0.1.0",
    "prop-types": "^15.5.8",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "redux": "^3.6.0",
    "redux-form": "^7.0.3",
    "redux-saga": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jest": "^19.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^19.0.2",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.4",
    "postcss": "^5.2.17",
    "postcss-cssnext": "^2.10.0",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "standard-react": "^4.0.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "retry.js",
      "/app/e2e/publish/"
    ],
    "globals": [
      "it",
      "console",
      "protractor",
      "browser",
      "by",
      "element",
      "expect",
      "jest",
      "describe",
      "assert",
      "test"
    ]
  },
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(css)$": "jest-css-modules"
    },
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/src/index.js",
      "app/storybook.js",
      "spec.js",
      ".stories.js",
      "/assets/",
      "/sagas/"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "babel": {
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
}
