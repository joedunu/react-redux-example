{
  "name": "y",
  "version": "0.0.1",
  "description": "Example implementation of react-redux",
  "main": "index.js",
  "engines": {
    "node": ">=6.9.2"
  },
  "scripts": {
    "start": "webpack-dev-server",
    "build": "webpack --config ./webpack.config.js",
    "test": "NODE_ENV=test jest && istanbul check-coverage",
    "lint": "standard --verbose | snazzy",
    "coverage": "istanbul check-coverage",
    "coveralls": "cat ./reports/coverage/lcov.info | node node_modules/.bin/coveralls"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/joedunu/react-redux-example.git"
  },
  "keywords": [
    "react",
    "redux"
  ],
  "author": "Anuradha Dunuhappawa",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/joedunu/react-redux-example/issues"
  },
  "homepage": "https://github.com/joedunu/react-redux-example#readme",
  "dependencies": {
    "axios": "^0.16.1",
    "classnames": "^2.2.5",
    "express": "^4.15.2",
    "history": "^4.7.2",
    "istanbul": "^0.4.5",
    "jest-babel-istanbul": "^0.1.0",
    "lodash": "^4.17.4",
    "material-ui": "^1.0.0-beta.22",
    "material-ui-icons": "^1.0.0-beta.17",
    "prop-types": "^15.5.8",
    "react": "^15.6.1",
    "react-dom": "^15.5.4",
    "react-modal": "^1.7.7",
    "react-redux": "^5.0.4",
    "react-router": "^4.1.1",
    "react-router-dom": "^4.1.1",
    "react-router-redux": "^5.0.0-alpha.9",
    "redux": "^3.6.0",
    "redux-form": "^7.0.3",
    "redux-saga": "^0.14.7"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-istanbul": "^0.12.2",
    "babel-jest": "^19.0.0",
    "babel-loader": "^6.4.1",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-es3-member-expression-literals": "^6.22.0",
    "babel-plugin-transform-es3-property-literals": "^6.22.0",
    "babel-polyfill": "^6.23.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-es2017": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.17",
    "copy-webpack-plugin": "^4.2.3",
    "coveralls": "^3.0.0",
    "css-loader": "^0.28.0",
    "enzyme": "^2.8.2",
    "enzyme-to-json": "^1.5.1",
    "eslint": "^3.19.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-config-standard-jsx": "^4.0.1",
    "eslint-config-standard-react": "^4.3.0",
    "eslint-loader": "^1.7.1",
    "eslint-plugin-jest": "^19.0.1",
    "extract-text-webpack-plugin": "^2.1.0",
    "html-webpack-plugin": "^2.28.0",
    "jest": "^21.2.1",
    "jest-css-modules": "^1.1.0",
    "json-loader": "^0.5.4",
    "jss-vendor-prefixer": "^7.0.0",
    "postcss": "^6.0.14",
    "postcss-cssnext": "^2.10.0",
    "postcss-extend": "^1.0.5",
    "postcss-import": "^9.1.0",
    "postcss-loader": "^1.3.3",
    "postcss-nested": "^3.0.0",
    "react-addons-test-utils": "^15.5.1",
    "react-test-renderer": "^15.5.4",
    "redux-mock-store": "^1.3.0",
    "resolve-url-loader": "^2.2.1",
    "snazzy": "^7.0.0",
    "standard": "^10.0.2",
    "standard-react": "^4.0.0",
    "style-loader": "^0.16.1",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "standard": {
    "ignore": [
      "/node_modules/",
      "retry.js",
      "/app/e2e/publish/"
    ],
    "globals": [
      "it",
      "console",
      "protractor",
      "browser",
      "by",
      "element",
      "expect",
      "jest",
      "describe",
      "assert",
      "test"
    ]
  },
  "jest": {
    "verbose": false,
    "collectCoverage": true,
    "moduleNameMapper": {
      "\\.(css)$": "jest-css-modules"
    },
    "collectCoverageFrom": [
      "app/**/*.js"
    ],
    "coveragePathIgnorePatterns": [
      "app/src/index.js",
      "app/storybook.js",
      "spec.js",
      ".stories.js",
      "/assets/",
      "/sagas/"
    ],
    "transform": {
      "^.+\\.js?$": "babel-jest"
    }
  },
  "babel": {
    "ignore": "node_modules",
    "presets": [
      "react",
      [
        "es2015",
        {
          "modules": false,
          "loose": true
        }
      ]
    ],
    "env": {
      "test": {
        "plugins": [
          "transform-es2015-modules-commonjs"
        ]
      }
    }
  }
}
